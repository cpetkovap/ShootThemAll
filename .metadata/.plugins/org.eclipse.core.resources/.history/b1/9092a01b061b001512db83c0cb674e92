package model;

public class PointsBooster extends Booster{
	private int points;	

	public PointsBooster(int points) {
		super(3, 3000, "Add points.");
		this.points = points;
	}

	public int getPoints() {
		return points;
	}

	private void setPoints(int points) {
		if(points > 0){
			this.points = points;
		}else{
			this.points = 10;
		}
	}
	
	

}
