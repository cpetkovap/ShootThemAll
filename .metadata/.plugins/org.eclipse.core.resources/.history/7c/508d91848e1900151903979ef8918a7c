package controller;

import model.Enemy;

public class EnemyFactory {
	
	public void getEnemy(int type){
		Enemy result = null;
		switch(type){
		case 1 : result = new Enemy(type, 1, 500, 1); break;
		case 2 : result = new Enemy(type, 2, 300, 2); break;
		case 3 : result = new Enemy(type, 3, 100, 3); break;
		default : result = new Enemy(100, health, duration, damage)
		}
		
	}

}
